  },
  "clients": [
    {
      "clientId": "techdados",
      "name": "TechDados",
      "enabled": true,
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "serviceAccountsEnabled": false,
      "redirectUris": ["http://localhost:13000/*", "http://localhost:3000/*"],
      "webOrigins": ["http://localhost:13000", "http://localhost:3000"],
      "protocol": "openid-connect",
      "attributes": {
        "pkce.code.challenge.method": "S256"
