# TechDados BFF - Environment Variables
# Copy to .env and adjust as needed

# App
APP_NAME=techdados-api
APP_VERSION=0.1.0
APP_ENV=local
DEBUG=false

# Demo mode - allows endpoints without JWT auth
DEMO_MODE=true

# API
API_PREFIX=/api

# Keycloak (OIDC)
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=techdados
KEYCLOAK_AUDIENCE=techdados-api

# Provider (API de Dados Techdengue)
# Leave empty to use fixture data
PROVIDER_BASE_URL=
PROVIDER_TIMEOUT_SECONDS=15

# Cache
CACHE_ENABLED=true
CACHE_TTL_SECONDS_P0=3600
CACHE_TTL_SECONDS_P1=3600

# CORS (comma-separated if multiple)
# CORS_ORIGINS=http://localhost:13000,http://localhost:5173
